
<div class="signup_form">
  <%= form_for(@checklist) do |f| %>
    <div><%= f.label :name %>
    <%= f.text_field :name %></div>

    <div><%= f.label :description %>
    <%= f.text_field :description %></div>

  <div class="form-remember">
    <%= f.check_box :private %> <%= f.label :private %>
</div>
    <div><%= f.button :Create,class: 'btn btn-primary' %></div>
  <% end %>
</div>
